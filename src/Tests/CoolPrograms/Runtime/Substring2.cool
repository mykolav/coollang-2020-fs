class Main() {
    var io: IO = new IO();

    def out_quoted_string_nl(s: String): Unit = {
        io.out_string("\""); 
        
        io.out_string(s); 
        
        io.out_string("\""); 
        io.out_nl();

        ()
    };

    {
        var s1: String = "Hello, World!";
        out_quoted_string_nl(s1.substring(-1, s1.length()));

        ()
    };
}

